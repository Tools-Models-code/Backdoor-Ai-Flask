# Backdoor AI Configuration

# LLM Provider Selection
# Options: together, openai, anthropic, google, mistral, cohere, ollama, custom
LLM_PROVIDER=ollama

# API Keys for Various Providers
TOGETHER_API_KEY=your_together_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
CUSTOM_API_KEY=your_custom_api_key_here

# Ollama Configuration (Local LLM)
# No API key needed for Ollama
OLLAMA_MODEL=llama4:latest
OLLAMA_API_BASE=http://localhost:11434

# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
SECRET_KEY=your_secret_key_here_or_it_will_be_generated

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/backdoor_db

# MCP Configuration
MCP_ENABLED=true
MCP_LOG_LEVEL=INFO

# VS Code Integration
VSCODE_SERVER_PATH=/usr/bin/code-server
VSCODE_BASE_URL=http://localhost
